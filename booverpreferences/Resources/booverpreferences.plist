			<key>cell</key>			<string>PSSwitchCell</string>
			<key>id</key> 			<string>EnableBooverToggle</string>
			<key>defaults</key>		<string>com.jontelang.boover</string>
			<key>key</key> 			<string>isEnabled</string>
			<key>label</key>		<string>Enable</string>
			<key>enabled</key> 		<true/>
			<key>default</key> 		<false/>
			<key>alternateColors</key><true/>
		</dict>

		<!--dict>
			<key>cell</key>			<string>PSSwitchCell</string>
			<key>default</key> 		<false/>

		<dict>
			<!--key>PostNotification</key><string>com.aaronash.threedeeboard-Settings</string-->
			<key>cell</key> 		<string>PSLinkListCell</string>
			<!--key>default</key> 		<integer>0</integer-->
			<key>defaults</key> 	<string>com.jontelang.boover</string>
			<key>key</key> 			<string>BadgePosition</string>
			<key>label</key> 		<string>Badge position</string>
			<key>detail</key> 		<string>PSListItemsController</string>
			<key>default</key> 		<integer>2</integer>
			<key>validValues</key> 	<array>
										<integer>0</integer>
										<integer>1</integer>
										<integer>2</integer>

										<integer>3</integer>
										<integer>4</integer>
										<integer>5</integer>

										<integer>6</integer>
										<integer>7</integer>
										<integer>8</integer>

										<!--integer>9</integer-->
									</array>
			<key>validTitles</key> 	<array>
										<string>Top left</string>
										<string>Top center</string>
										<string>Top right (normal)</string>

										<string>Middle left</string>

			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Move right</string>
			<key>action</key>  		<string>increasex</string>
		</dict>
		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Move left</string>
			<key>action</key>  		<string>decreasex</string>
		</dict>


		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>footerText</key> 	<string>For now, the below settings are iOS 5 and 6 only.</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>label</key>		<string>Alpha</string>
		</dict>
		<dict>
			<key>cell</key>			<string>PSSliderCell</string>
			<key>key</key>			<string>alpha</string>
			<key>defaults</key>     <string>com.jontelang.boover</string>
			<key>showValue</key> 	<true/>
			<key>min</key> 			<integer>0</integer>
			<key>max</key> 			<integer>1</integer>
			<key>id</key> 			<string>SliderAlpha</string>
			<key>default</key> 		<integer>1</integer>
		</dict>

		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>label</key>		<string>Badge scale</string>
		</dict>

			<key>cell</key>			<string>PSSliderCell</string>
			<key>key</key>			<string>scale</string>
			<key>defaults</key>     <string>com.jontelang.boover</string>
			<key>showValue</key> 	<true/>
			<key>min</key> 			<integer>-1</integer>
			<key>max</key> 			<integer>3</integer>
			<key>id</key> 			<string>SliderScale</string>
			<key>default</key> 		<integer>1</integer>
		</dict>
		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Reset to 1</string>
			<key>action</key>  		<string>resetscale</string>
		</dict>
		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Make bigger</string>
			<key>action</key>  		<string>increasescale</string>
		</dict>
		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Make smaller</string>
			<key>action</key>  		<string>decreasescale</string>
